  "postAttachCommand": "bash scripts/fetch.sh || true",
  "containerEnv": { "GITHUB_TOKEN": "", "GH_TOKEN": "" },
  "customizations": { "vscode": { "settings": { "terminal.integrated.defaultProfile.linux": "bash" } } }
}
